@model Domains.TbEmployeeEvaluation

<div class="app-content content">
        <div class="content-overlay"></div>
        <div class="header-navbar-shadow"></div>
        <div class="content-wrapper">
            <div class="content-header row">
                <div class="content-header-left col-md-9 col-12 mb-2">
                    <div class="row breadcrumbs-top">
                        <div class="col-12">
                           
                        </div>
                    </div>
                </div>
              
            </div>
            <div class="content-body">
              
                <!-- // Basic multiple Column Form section start -->
                <section id="multiple-column-form">
                    <div class="row match-height">
                        <div class="col-12">
                            <div class="card">
                                <div class="card-header">
                                    <h4 class="card-title">Client</h4>
                                </div>
                                <div class="card-content">
                                    <div class="card-body">
                                    <form enctype="multipart/form-data" method="post" role="form" asp-controller="EmployeeEvaluation" asp-action="Save" class="form">
                                            <div class="form-body">
                                                <div class="row">
                                                <div class="col-sm-6 col-12" data-select2-id="127">


                                                    <div class="form-group" data-select2-id="126">
                                                        <input asp-for="EmployeeEvaluationId" type="hidden" class="form-control">
                                                        <select class="select2 form-control select2-hidden-accessible" asp-for="EmployeeId" asp-items="@(new SelectList(ViewBag.Employees , "EmployeeId" , "EmplyeeName"))" data-select2-id="1" tabindex="-1" aria-hidden="true">
                                                        </select>
                                                    </div>
                                                </div>
                                                    <div class="col-md-6 col-12">
                                                        <div class="form-label-group">
                                                          
                                                        <input type="text" id="first-name-column" class="form-control" placeholder="Evaluation Syntax" asp-for="EvaluationSyntax">
                                                        <label for="first-name-column"> Evaluation Syntax</label>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-6 col-12">
                                                        <div class="form-label-group">
                                                        <input type="text" id="last-name-column" class="form-control" placeholder="Evaluation Numerical" asp-for="EvaluationNumerical">
                                                        <label for="last-name-column">Evaluation Numerical</label>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-6 col-12">
                                                        <div class="form-label-group">
                                                        <input type="text" id="city-column" class="form-control" placeholder="Employee Basic WorkHours" asp-for="EmployeeBasicWorkHours">
                                                        <label for="city-column">Employee Basic WorkHours</label>
                                                        </div>
                                                    </div>
                                                <div class="col-md-6 col-12">
                                                    <div class="form-label-group">
                                                        <input type="text" id="city-column" class="form-control" placeholder="Employee Extra Work Hours" asp-for="EmployeeExtraWorkHours">
                                                        <label for="city-column">Employee Extra Work Hours</label>
                                                    </div>
                                                </div>
                                               
                                               
                                                  
                                                  
                                                    <div class="col-12">
                                                    <button  type="submit" class="btn btn-primary mr-1 mb-1 waves-effect waves-light">Submit</button>
                                                       
                                                    </div>
                                                </div>
                                            </div>
                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </section>
                <!-- // Basic Floating Label Form section end -->

            </div>
        </div>
    </div>











































